<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendancemanage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AttendanceManage">
        <activity
            android:name=".ViewStudentByBranchYear"
            android:label="@string/title_activity_view_student_by_branch_year"
            android:theme="@style/Theme.AttendanceManage.NoActionBar"></activity>
        <activity
            android:name=".View_Attendance"
            android:label="@string/title_activity_view__attendance"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity android:name=".Faculty" />
        <activity
            android:name=".ViewFaculty"
            android:label="@string/title_activity_view_faculty"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".Add_Faculty"
            android:label="@string/title_activity_add__faculty"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".ViewStudent"
            android:label="@string/title_activity_view_student"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".Add_Student"
            android:label="@string/title_activity_add__student"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".AddAttendance"
            android:label="@string/title_activity_add_attendance"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".Admin"
            android:label="@string/title_activity_admin"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.AttendanceManage.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>